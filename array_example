public class array_example {
    public static void main(String[] args) {
        //to reverse the content of an array
        int[] arr={10,20,30,40,50};
       int j=arr.length-1;
        int temp = 0; 
        for(int i=0; i<= (arr.length-1)/2; i++ ){
            temp=arr[i];

            arr[i]=arr[j];
            arr[j]=temp;
            j--;

        }
        for (int x:arr){
            System.out.println(x);
        }
    }
}
